{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": ".lc1",
    "patterns": [
        {
            "include": "#operator"
        },
        {
            "include": "#function"
        },
        {
            "include": "#comment"
        },
        {
            "include": "#string"
        }
    ],
    "repository": {
        "operator": {
            "patterns": [
                {
                    "name": "keyword.operator",
                    "match": "\\\\|\\.|0"
                }
            ]
        },
        "function": {
            "patterns": [
                {
                    "name": "entity.name.function",
                    "match": "\\b(S)\\b"
                }
            ]
        },
        "comment": {
            "patterns": [
                {
                    "begin": "//",
                    "end": "$",
                    "name": "comment.line.character"
                }
            ]
        },
        "string": {
            "patterns": [
                {
                    "match": "((?:\"(?:[^\"]|\")*\"))",
                    "name": "string.quoted"
                }
            ]
        }
    },
    "scopeName": "source.199365597"
}