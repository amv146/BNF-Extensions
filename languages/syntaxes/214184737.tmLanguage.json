{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Lambda Nat 5",
    "patterns": [
        {
            "include": "#blockComment"
        },
        {
            "include": "#comment"
        },
        {
            "include": "#function"
        },
        {
            "include": "#keyword"
        },
        {
            "include": "#operator"
        },
        {
            "include": "#separator"
        }
    ],
    "repository": {
        "blockComment": {
            "patterns": [
                {
                    "begin": "/\\*",
                    "end": "\\*/",
                    "name": "comment.block"
                }
            ]
        },
        "comment": {
            "patterns": [
                {
                    "begin": "//",
                    "end": "$",
                    "name": "comment.line.character"
                }
            ]
        },
        "function": {
            "patterns": [
                {
                    "name": "entity.name.function",
                    "match": "\\b(fix)\\b"
                }
            ]
        },
        "keyword": {
            "patterns": [
                {
                    "name": "keyword.control",
                    "match": "\\b(let|rec|in)\\b"
                }
            ]
        },
        "operator": {
            "patterns": [
                {
                    "name": "keyword.operator",
                    "match": "="
                }
            ]
        },
        "separator": {
            "patterns": [
                {
                    "name": "punctuation.separator",
                    "match": ";;"
                }
            ]
        }
    },
    "scopeName": "source.214184737"
}