{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Lambda Nat 5",
    "patterns": [
        {
            "include": "#keyword"
        },
        {
            "include": "#number"
        }
    ],
    "repository": {
        "keyword": {
            "patterns": [
                {
                    "name": "keyword.control",
                    "match": "\\b(let|in|true|false|if|then|else|succ|pred|iszero|fix)\\b"
                }
            ]
        },
        "number": {
            "patterns": [
                {
                    "match": "((?:0x[0-9a-fA-F]+)|(?:0b[01]+)|(?:\\d+\\.\\d+)|(?:\\d+))",
                    "name": "constant.numeric"
                }
            ]
        }
    },
    "scopeName": "source.199176507"
}